{
  "name": "nextfire-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "firebase emulators:start --export-on-exit=emulatordata --import=emulatordata & next dev",
    "dev:prod-data": "PROD_DATA=true next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && next lint",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "clean": "rm -rf node_modules && rm -rf .next"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0 <19.0.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.9",
    "csstype": "^3.0.10",
    "firebase": "^9.17.1",
    "framer-motion": "^8.5.4",
    "gsap": "^3.11.4",
    "image-url-validator": "^1.0.4",
    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "^4.1.1",
    "next": "^13.1.6",
    "react": "18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.43.0",
    "react-hot-toast": "^2.4.0",
    "react-markdown": "^8.0.5",
    "react-modal": "^3.16.1",
    "react-overlays": "^5.2.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/react": "^18.0.27",
    "@types/react-modal": "^3.13.1",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.5.1",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-tailwindcss": "^3.8.3",
    "firebase-tools": "^11.22.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "react-tooltip": "^5.7.4",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.9.4"
  }
