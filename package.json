{
  "name": "nextfire-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "firebase emulators:start --export-on-exit=emulatordata --import=emulatordata & next dev",
    "dev:prod-data": "PROD_DATA=true next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "clean": "rm -rf node_modules && rm -rf .next",
    "cy:open": "cypress open",
    "cy:test": "cypress run"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "csstype": "^3.2.3",
    "firebase": "^11.10.0",
    "framer-motion": "^12.33.0",
    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "^4.1.1",
    "next": "^15.5.7",
    "react": "19.2.4",
    "react-dom": "^19.2.4",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.71.1",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-modal": "^3.16.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.kebabcase": "^4.1.9",
    "@types/react": "^19.2.13",
    "@types/react-modal": "^3.16.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.24",
    "cypress": "^14.5.4",
    "eslint": "8.57.1",
    "eslint-config-next": "15.5.12",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "firebase-tools": "^15.5.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react-tooltip": "^5.30.0",
    "typescript": "^5.9.3",
    "tailwindcss": "^3.4.19"
  }
