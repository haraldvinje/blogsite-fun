{
  "name": "nextfire-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "firebase emulators:start --export-on-exit=emulatordata --import=emulatordata & next dev",
    "dev:prod-data": "PROD_DATA=true next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "clean": "rm -rf node_modules && rm -rf .next"
  },
  "engines": {
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "csstype": "^3.1.3",
    "firebase": "^10.11.0",
    "framer-motion": "^11.1.7",
    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "^4.1.1",
    "next": "^14.2.2",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.51.3",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.0.1",
    "react-modal": "^3.16.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.12",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.kebabcase": "^4.1.9",
    "@types/react": "^18.2.79",
    "@types/react-modal": "^3.16.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.8.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "postcss": "^8.4.38",
    "firebase-tools": "^13.7.3",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react-tooltip": "^5.26.3",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  }
}
