{
  "name": "nextfire-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "firebase emulators:start --export-on-exit=emulatordata --import=emulatordata & next dev",
    "dev:prod-data": "PROD_DATA=true next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && next lint",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "clean": "rm -rf node_modules && rm -rf .next"
  },
  "engines": {
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "csstype": "^3.1.2",
    "firebase": "^9.23.0",
    "framer-motion": "^10.12.17",
    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "^4.1.1",
    "next": "^13.4.7",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.45.0",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^8.0.7",
    "react-modal": "^3.16.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/react": "^18.2.14",
    "@types/react-modal": "^3.16.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.15.0",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "firebase-tools": "^12.4.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react-tooltip": "^5.15.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  }
}
