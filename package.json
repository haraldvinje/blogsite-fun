{
  "name": "nextfire-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "firebase emulators:start --export-on-exit=emulatordata --import=emulatordata & next dev",
    "dev:prod-data": "PROD_DATA=true next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && next lint",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "clean": "rm -rf node_modules && rm -rf .next"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0 <19.0.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "csstype": "^3.0.10",
    "firebase": "^9.15.0",
    "framer-motion": "^6.2.8",
    "gsap": "^3.9.1",
    "image-url-validator": "^1.0.4",
    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "^4.1.1",
    "next": "^12.2.5",
    "react": "18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.41.0",
    "react-hot-toast": "^2.4.0",
    "react-markdown": "^8.0.4",
    "react-modal": "^3.16.1",
    "react-overlays": "^5.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.8",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/react": "^18.0.26",
    "@types/react-modal": "^3.13.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^9.5.0",
    "eslint": "8.30.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "firebase-tools": "^11.19.0",
    "postcss": "^8.4.20",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "react-tooltip": "^4.2.21",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.9.4"
  }
