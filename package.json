{
  "name": "nextfire-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "firebase emulators:start --export-on-exit=emulatordata --import=emulatordata & next dev",
    "dev:prod-data": "PROD_DATA=true next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "clean": "rm -rf node_modules && rm -rf .next",
    "cy:open": "cypress open",
    "cy:test": "cypress run"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "csstype": "^3.1.3",
    "firebase": "^10.14.1",
    "framer-motion": "^11.17.0",
    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "^4.1.1",
    "next": "^14.2.15",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.1",
    "react-markdown": "^9.0.1",
    "react-modal": "^3.16.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.kebabcase": "^4.1.9",
    "@types/react": "^18.3.12",
    "@types/react-modal": "^3.16.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.17.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "firebase-tools": "^13.29.1",
    "postcss": "^8.4.48",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react-tooltip": "^5.28.0",
    "typescript": "^5.7.2",
    "tailwindcss": "^3.4.17"
  }
