{
  "name": "nextfire-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "firebase emulators:start --export-on-exit=emulatordata --import=emulatordata & next dev",
    "dev:prod-data": "PROD_DATA=true next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && next lint",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "clean": "rm -rf node_modules && rm -rf .next"
  },
  "engines": {
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "csstype": "^3.1.2",
    "framer-motion": "^10.16.4",
    "firebase": "^10.4.0",
    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "^4.1.1",
    "next": "^13.5.2",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.46.2",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^8.0.7",
    "react-modal": "^3.16.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/react": "^18.2.22",
    "@types/react-modal": "^3.16.0",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.3.0",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8.4.30",
    "firebase-tools": "^12.5.4",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "react-tooltip": "^5.21.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  }
}
