    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "^4.1.1",
    "next": "^12.2.5",
    "react": "18.1.0",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.0.2",
    "react-hook-form": "^7.27.1",
